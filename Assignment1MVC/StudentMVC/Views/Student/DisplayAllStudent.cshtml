<table class="table table-bordered table-hover">
    @foreach (Student item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Address</td>
            <td><img src="/Image/@item.Image" style="width:50px; height:50px;"></td>
            <td><a href="/student/details/@item.Id">Details</a></td>
        </tr>
    }
</table>